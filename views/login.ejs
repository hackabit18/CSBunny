<!DOCTYPE html>
<html lang="en" >

<head>
<meta charset="UTF-8">
  <title>SignUp/Login</title>
  <link href='https://fonts.googleapis.com/css?family=Titillium+Web:400,300,600' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
  
  <link rel="stylesheet" href="userLP/css/style.css">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1">
  <link rel="shortcut icon" href="assets/images/logo2.png" type="image/x-icon">
  <meta name="description" content="">
  <link rel="stylesheet" href="assets/web/assets/mobirise-icons/mobirise-icons.css">
  <link rel="stylesheet" href="assets/tether/tether.min.css">
  <link rel="stylesheet" href="assets/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="assets/bootstrap/css/bootstrap-grid.min.css">
  <link rel="stylesheet" href="assets/bootstrap/css/bootstrap-reboot.min.css">
  <link rel="stylesheet" href="assets/socicon/css/styles.css">
  <link rel="stylesheet" href="assets/dropdown/css/style.css">
  <link rel="stylesheet" href="assets/theme/css/style.css">
  <link rel="stylesheet" href="assets/mobirise/css/mbr-additional.css" type="text/css">

</head>

<body>
    <br>
    <br>
    <br>
    <br>
  <div class="container">
  <form class="signUp" method ="post" action ='/'>
    <h3>Sign Up</h3>
    
   <!-- <input type="text" value="CUSTOMER" disabled/> -->
    <br>
    <input type="text" name ="userName" placeholder="UserName" reqired />
    <br>
    <br>
    <input type="text" name ="email" placeholder="Email" reqired />
    <br>
    <br>
    <input type="text" name ="name" placeholder="Name" reqired />
    <br>
    <br>
    <input type="number" name ="phone" placeholder="Phone" reqired />
    <br>
    <br>
    <p>Skill</p>
    <select>
      <option value ="HTML">HTML</option>
      <option value ="CSS">CSS</option>
      <option value ="JS">JS</option>
    </select>
    
    <input type="hidden" name="parameter[]" value="Skill" reqired />
    <input type="password" name ="password" placeholder="Password" reqired />
    <br>
    <br>
    <button class="form-btn sx log-in" type="button">Log In</button>
    <button class="form-btn dx" type="submit">Sign Up</button>
  </form>
  <form class="signIn" method ="post" action ='/'>
    <h3>Welcome Back !</h3>
    <br>
    <br>
    <input type="text" placeholder="UserName" name="userName" autocomplete='off' reqired />
    <br>
    <br>
    <input type="password" placeholder="Password" name="password" reqired />
    <br><br>
    <button class="form-btn sx back" type="button">Back</button>
    <button class="form-btn dx" type="submit">Log In</button>
  </form>
</div>
  <script src='https://code.jquery.com/jquery-2.2.4.min.js'></script>
  <script  src="userLP/js/index.js"></script>
</body>
</html>